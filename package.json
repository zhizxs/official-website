{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve  --open --mode dev",
    "prod": "vue-cli-service build --mode prod",
    "pre": "vue-cli-service build --mode pre",
    "sit": "vue-cli-service build --mode sit"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "element-ui": "^2.13.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^5.1.1",
    "sass-loader": "^8.0.2",
    "swiper": "^5.4.1",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-drawer-layout": "^1.3.0",
    "vue-router": "^3.1.6",
    "vue-swiper-component": "^2.1.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "prettier": {
    "printWidth": 1000
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "commonjs": true,
      "es6": true,
      "amd": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "document": "readonly",
      "localStorage": "readonly",
      "window": "readonly",
      "_g": "readonly",
      "userService": "readonly",
      "orderService": "readonly",
      "publicjs": "readonly",
      "myMsg": "readonly",
      "myMsgBox": "readonly",
      "WebViewJavascriptBridge": "readonly",
      "wx": "readonly",
      "WeixinJSBridge": "readonly",
      "_czc": "readonly"
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "semi": 0,
      "array-element-newline": 0,
      "max-len": 0,
      "multiline-ternary": 0,
      "newline-per-chained-call": 0,
      "linebreak-style": 0,
      "no-useless-escape": 0,
      "no-redeclare": 0,
      "no-empty": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
