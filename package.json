{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve  --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^2.6.11",
    "element-ui": "^2.13.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "prettier": {
    "printWidth": 1000
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "commonjs": true,
      "es6": true,
      "amd": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "document": "readonly",
      "localStorage": "readonly",
      "window": "readonly",
      "_g": "readonly",
      "userService": "readonly",
      "orderService": "readonly",
      "publicjs": "readonly",
      "myMsg": "readonly",
      "myMsgBox": "readonly",
      "WebViewJavascriptBridge": "readonly",
      "wx": "readonly",
      "WeixinJSBridge": "readonly",
      "_czc": "readonly"
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "semi": 0,
      "array-element-newline": 0,
      "max-len": 0,
      "multiline-ternary": 0,
      "newline-per-chained-call": 0,
      "linebreak-style": 0,
      "no-useless-escape": 0,
      "no-redeclare": 0,
      "no-empty": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
